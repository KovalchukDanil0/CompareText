{
  "manifest_version": 3,
  "name": "Compare Text",
  "author": "Gomofob",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_title": "Click to open popup",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": ["storage", "contextMenus"]
}
